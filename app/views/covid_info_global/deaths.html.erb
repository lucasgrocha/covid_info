<h1>Deaths</h1>

<table class="table table-hover table-bordered">
  <thead class="thead-dark">
    <tr>
      <th>Country</th>
      <th>City</th>
      <th>Deaths</th>
      <th>Last update</th>
    </tr>
  </thead>

  <tbody>
    <% @deaths.each do |death| %>
      <tr>
        <td><%= death['countryRegion'] %></td>
        <td><%= death['provinceState'] %></td>
        <td><%= death['deaths'] %></td>
        <td><%= parse_time(death['lastUpdate']) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
