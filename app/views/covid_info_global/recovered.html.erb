<h1>Recovered</h1>

<table>
  <thead>
    <td>Country</td>
    <td>City</td>
    <td>Recovered</td>
    <td>Last update</td>
  </thead>

  <tbody>
    <% @recovered.each do |recovered| %>
      <tr>
        <td><%= recovered['countryRegion'] %></td>
        <td><%= recovered['provinceState'] %></td>
        <td><%= recovered['recovered'] %></td>
        <td><%= parse_time(recovered['lastUpdate']) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
